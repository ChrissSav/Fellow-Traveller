<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/orange_new" />
            <size
                android:width="60dp"
                android:height="60dp" />

        </shape>

    </item>
    <item
        android:width="30dp"
        android:height="30dp"
        android:gravity="center">
        <rotate
            android:fromDegrees="180"
            android:gravity="center"
            android:drawable="@drawable/ic_back_button_white" />
    </item>

</layer-list>