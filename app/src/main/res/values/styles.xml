    <style name="CustomProgressBar">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/progress_register_background</item>
        <item name="android:minHeight">10dip</item>
        <item name="android:maxHeight">20dip</item>
    </style>

    <style name="CustomProgressBarOrange">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/progress_register_background_orange</item>
        <item name="android:minHeight">10dip</item>
        <item name="android:maxHeight">20dip</item>
    </style>

    <style name="DialogCostumeTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">@color/blue_offer</item>
    </style>


    <style name="ThemeDialogNew" parent="android:Theme.Holo.Dialog">
        <item name="android:windowMinWidthMajor">100%</item>
        <item name="android:windowMinWidthMinor">100%</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>


    <style name="Theme_Dialog" parent="android:Theme.Holo.Dialog">
        <item name="android:windowMinWidthMajor">100%</item>
        <item name="android:windowMinWidthMinor">100%</item>
        android:fromYDelta="50%p" android:toYDelta="0%p"
        android:duration="@android:integer/config_longAnimTime"
        <item name="android:windowBackground">@color/white</item>
        <item name="android:textColor">@color/black_color</item>
    </style>

    <style name="Theme_Dialog_Abort" parent="android:Theme.Holo.Dialog">
        <item name="android:windowMinWidthMajor">80%</item>
        <item name="android:windowMinWidthMinor">80%</item>
        <item name="android:fromYDelta">50%</item>
        <item name="android:toYDelta">0%</item>
        <item name="android:duration">@android:integer/config_longAnimTime</item>
        <item name="android:windowBackground">@color/white</item>
        <item name="android:textColor">@color/black_color</item>
    </style>

    <style name="MyCustomTabLayoutTextAppearance" parent="TextAppearance.Design.Tab">
        <item name="textAllCaps">false</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontFamily">@font/firasans_bold</item>
        <item name="android:textColor">@color/black_color</item>
        <item name="android:textSize">20sp</item>
    </style>
