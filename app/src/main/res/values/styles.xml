<resources xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <color name="white_opaque">#FFFFFFFF</color>
    <color name="pitch_black">#000000</color>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="android:statusBarColor">@color/black_new</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="colorPrimary">@color/black_new</item>
        <item name="android:navigationBarColor">@color/black_new</item>
        <item name="colorPrimaryDark">@color/orange_color</item>
        <item name="colorAccent">@color/orange_color</item>
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@color/black_new</item>
        <item name="android:colorBackground">@color/black_new</item>
        <item name="colorSecondary">@color/home_blue</item>
        <item name="colorError">@color/home_blue</item>
        <!-- New MaterialComponents attributes. -->
        <item name="colorPrimaryVariant">@color/black_new</item>
        <item name="colorSecondaryVariant">@color/black_new</item>
        <item name="colorSurface">@color/white</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorOnSecondary">@color/home_blue</item>
        <item name="colorOnBackground">@color/black_new</item>
        <item name="colorOnError">@color/red_color</item>
        <item name="colorOnSurface">@color/black_new</item>
        <item name="scrimBackground">@color/black_new</item>
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>
        <item name="textAppearanceHeadline1">@style/TextAppearance.MaterialComponents.Headline1
        </item>
        <item name="textAppearanceHeadline2">@style/TextAppearance.MaterialComponents.Headline2
        </item>
        <item name="textAppearanceHeadline3">@style/TextAppearance.MaterialComponents.Headline3
        </item>
        <item name="textAppearanceHeadline4">@style/TextAppearance.MaterialComponents.Headline4
        </item>
        <item name="textAppearanceHeadline5">@style/TextAppearance.MaterialComponents.Headline5
        </item>
        <item name="textAppearanceHeadline6">@style/TextAppearance.MaterialComponents.Headline6
        </item>
        <item name="textAppearanceSubtitle1">@style/TextAppearance.MaterialComponents.Subtitle1
        </item>
        <item name="textAppearanceSubtitle2">@style/TextAppearance.MaterialComponents.Subtitle2
        </item>
        <item name="textAppearanceBody1">@style/TextAppearance.MaterialComponents.Body1</item>
        <item name="textAppearanceBody2">@style/TextAppearance.MaterialComponents.Body2</item>
        <item name="textAppearanceCaption">@style/TextAppearance.MaterialComponents.Caption</item>
        <item name="textAppearanceButton">@style/TextAppearance.MaterialComponents.Button</item>
        <item name="textAppearanceOverline">@style/TextAppearance.MaterialComponents.Overline</item>
        <item name="materialCalendarFullscreenTheme">
            @style/CustomThemeOverlay_MaterialCalendar_Fullscreen
        </item>
    </style>

    <style name="Full" parent="AppTheme">
        <!--this property will help hide the ActionBar-->
        <item name="android:statusBarColor">#242225</item>

    </style>

    <!-- New MaterialComponents styles for full screen -->
    <style name="CustomThemeOverlay_MaterialCalendar_Fullscreen" parent="@style/ThemeOverlay.MaterialComponents.MaterialCalendar.Fullscreen">
        <item name="materialCalendarStyle">@style/Custom_MaterialCalendar.Fullscreen</item>
        <item name="android:overScrollMode">never</item>
    </style>

    <style name="Custom_MaterialCalendar.Fullscreen" parent="@style/Widget.MaterialComponents.MaterialCalendar.Fullscreen">
        <item name="android:windowFullscreen">false</item>
    </style>


    <style name="MySeekBar" parent="Widget.AppCompat.SeekBar">
        <item name="android:progressBackgroundTint">@color/grey</item>
        <item name="android:progressTint">@color/orange_color</item>
        <item name="android:colorControlActivated">@color/orange_color</item>
        <item name="android:colorControlNormal">@color/orange_color</item>
    </style>


    <style name="CustomProgressBar">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/progress_register_background</item>
        <item name="android:minHeight">10dip</item>
        <item name="android:maxHeight">20dip</item>
    </style>

    <style name="CustomProgressBarOrange">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/progress_register_background_orange</item>
        <item name="android:minHeight">10dp</item>
        <item name="android:maxHeight">20dp</item>
    </style>

    <style name="DialogCostumeTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">@color/black_new</item>
    </style>


    <style name="ThemeDialogNew" parent="android:Theme.Holo.Dialog">
        <item name="android:windowMinWidthMajor">100%</item>
        <item name="android:windowMinWidthMinor">100%</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>


    <style name="Theme_Dialog" parent="android:Theme.Holo.Dialog">
        <item name="android:windowMinWidthMajor">100%</item>
        <item name="android:windowMinWidthMinor">100%</item>
        <item name="android:fromYDelta">50%</item>
        <item name="android:toYDelta">0%</item>
        <item name="android:duration">@android:integer/config_longAnimTime</item>
        <item name="android:windowBackground">@color/white</item>
        <item name="android:textColor">@color/black_new</item>
    </style>

    <style name="Theme_Dialog_Abort" parent="android:Theme.Holo.Dialog">
        <item name="android:windowMinWidthMajor">80%</item>
        <item name="android:windowMinWidthMinor">80%</item>
        <item name="android:fromYDelta">50%</item>
        <item name="android:toYDelta">0%</item>
        <item name="android:duration">@android:integer/config_longAnimTime</item>
        <item name="android:windowBackground">@color/white</item>
        <item name="android:textColor">@color/black_new</item>
    </style>


    <style name="MyCustomTabLayoutTextAppearance" parent="TextAppearance.Design.Tab">
        <item name="textAllCaps">false</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontFamily">@font/firasans_bold</item>
        <item name="android:textColor">@color/black_new</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="Full_screen_dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
    </style>


    <style name="CustomBottomSheetDialog" parent="@style/ThemeOverlay.MaterialComponents.BottomSheetDialog">
        <item name="bottomSheetStyle">@style/CustomBottomSheet</item>
    </style>

    <style name="CustomBottomSheet" parent="Widget.MaterialComponents.BottomSheet">
        <item name="shapeAppearanceOverlay">@style/CustomShapeAppearanceBottomSheetDialog</item>
    </style>

    <style name="CustomShapeAppearanceBottomSheetDialog" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">16dp</item>
        <item name="cornerSizeTopLeft">16dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeBottomLeft">0dp</item>
    </style>

    <style name="RoundedSquare">
        <item name="cornerSize">16%</item>
    </style>

    <style name="ImageShapeForTrip">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">16dp</item>
        <item name="cornerSizeTopLeft">16dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeBottomLeft">0dp</item>
    </style>

    <style name="roundedImageView" parent="">
        <item name="cornerSize">16dp</item>
    </style>
</resources>