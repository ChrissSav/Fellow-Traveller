<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="PickNumberButton">
        <attr name="mv_minusBtnDrawable" format="integer" />
        <attr name="mv_plusBtnDrawable" format="integer" />
        <attr name="mv_pickTxtColor" format="color" />
        <attr name="mv_pickTxtBackground" format="integer" />
        <attr name="minValue" format="integer" />
        <attr name="maxValue" format="integer" />
        <attr name="minus_src" format="reference|color" />
        <attr name="plus_src" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="FellowEditText">
        <attr name="drawable_right" format="reference|color" />
        <attr name="bottom_line_color" format="color" />
        <attr name="bottom_line_top_margin" format="dimension" />
        <attr name="drawable_padding" format="dimension" />
        <attr name="padding_end" format="dimension" />
        <attr name="padding_start" format="dimension" />
        <attr name="style" format="integer" />
        <attr name="input_type" format="string" />
        <attr name="hint_text" format="string" />
    </declare-styleable>


    <declare-styleable name="FellowTextInput">
        <attr name="hint_fellow" format="string" />
        <attr name="editable" format="boolean" />
        <attr name="imeOptions" format="string" />
    </declare-styleable>

    <declare-styleable name="LabelTextComponent">
        <attr name="label_text" format="string" />
    </declare-styleable>
</resources>